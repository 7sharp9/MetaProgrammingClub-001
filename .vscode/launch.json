    "version": "0.2.0",
    "configurations": [

        {
            "name": ".NET Core Launch (C# console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build C#",
            "program": "${workspaceFolder}/csharp/bin/Debug/netcoreapp2.2/MetaProgrammingClub-001.dll",
            "args": [],
            "cwd": "${workspaceFolder}/csharp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (F# console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build F#",
            "program": "${workspaceFolder}/fsharp/bin/Debug/netcoreapp2.2/MetaProgrammingClub-001.dll",
            "args": [],
            "cwd": "${workspaceFolder}/csharp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}